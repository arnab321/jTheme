<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <layer-list>
            <item android:drawable="@drawable/joyn_white_btn_rcs_video_call_disabled" />
        </layer-list>
    </item>
    <item android:state_pressed="true">
        <layer-list>
            <item android:drawable="@drawable/joyn_white_btn_rcs_video_call_pressed" />
        </layer-list>
    </item>
    <item android:state_focused="true">
        <layer-list>
            <item android:drawable="@drawable/joyn_white_btn_rcs_video_call_pressed" />
        </layer-list>
    </item>
    <item android:drawable="@drawable/joyn_white_btn_rcs_video_call_normal" />
</selector>